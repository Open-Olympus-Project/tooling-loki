config:
  storage_config:
    azure:
      container_name: "loki"
      account_name: "<account-name>"
      account_key: "<account-key>"
      download_buffer_size: 512000
      upload_buffer_size: 256000
      upload_buffer_count: 1
      request_timeout: 30s
      max_retries: 5
      min_retry_delay: 10ms
      max_retry_delay: 500ms
